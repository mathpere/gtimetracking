<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.googlecode.gtimetracking</groupId>
  <artifactId>deploy</artifactId>
  <packaging>pom</packaging>
  <version>1</version>

	<repositories>
		<repository>
			<id>maven2-repository.dev.java.net</id>
			<name>Java.net Repository for Maven</name>
			<url>http://download.java.net/maven/2/</url>
		</repository>
	</repositories>

	<!-- 
		define in your ~/.m2/settings.xml:
		<servers>
			<server>
				<id>googlecode</id>
				<username>googlecode.username</username>
				<password>googlecode.password</password>
			</server>
		</servers>
	-->

	<distributionManagement>
		<repository>
			<uniqueVersion>false</uniqueVersion>
			<id>googlecode</id>
			<url>svn:https://gtimetracking.googlecode.com/svn/repo</url>
		</repository>
	</distributionManagement>

	<build>
		<extensions>
			<extension>
				<groupId>org.jvnet.wagon-svn</groupId>
				<artifactId>wagon-svn</artifactId>
				<version>1.8</version>
			</extension>
		</extensions>
	</build>

</project>